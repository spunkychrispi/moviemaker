package scion.movie
{
	public class ScionEmbedDownloadProgressBar extends ScionDownloadProgressBar
	{
		
		public function ScionEmbedDownloadProgressBar() 
		{
			super();
		}
	
		
		protected override function center(width:Number, height:Number):void
		{
			// 265 is the applicationX value in application
			// 229 and 13 are the dimensions of the loader
			_xOffset = Math.floor((width-259) / 2);
			_yOffset = Math.floor((height-80) / 2);
		}  	     
	}
}
